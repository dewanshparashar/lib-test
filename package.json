  "scripts": {
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "dev": "concurrently \"yarn storybook\" \"yarn dev:tw\"",
    "dev:tw": "yarn build:tw --watch",
    "build:tw": "yarn tailwindcss -i ./src/main.css -o ./dist/build.css",
    "build:tw:prod": "yarn tailwindcss -i ./src/main.css -o ./dist/build.min.css --minify",
    "copyAssets": "cp -R static dist",
    "build": "rimraf dist && tsc && yarn build:tw:prod && yarn copyAssets",
    "test": "yarn jest",
    "test:coverage": "yarn jest --coverage",
    "test:watch": "yarn test --watch",
    "lint": "eslint 'src/**/*.{js,ts,tsx}'",
    "buildx": "rimraf dist && rollup -c --bundleConfigAsCjs && yarn build:tw:prod"
  },
  "exports": {
    ".": "./dist/index.js",
    "./cobalt.css": "./dist/build.min.css"
  },
  "engines": {
    "node": ">=14"
  }
}
